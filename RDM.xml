		<set name="Relic">
			<!-- Enhance Dia III and Slow II -->
			<head augment="M88671560">Viti. Chapeau +1</head> 
			<!-- Enhance Chainspell -->
			<body augment="M7428">Viti. Tabard +1</body>
			<!-- Enhance Phalanx II -->
			<hands augment="M1354">Vitiation Gloves +1</hands>
			<!-- Enhance Bio III and Blind II -->
