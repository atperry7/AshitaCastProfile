			<feet augment="S81364004147268">Taeon Boots</feet>

		</set>
		<include>
		</include>
	</sets>
	<variables>
		<!-- Spells Affected by Enhancing Magic Skill at time of cast -->
		<!-- Protect, Shell, Aquaveil, Stoneskin, Enspells, Barspells, Gain-Spells, Temper -->
		<var name="EnhanceTimeOfCast">Protect*|Shell*|Aquaveil|Stoneskin|En*|Bar*|Gain*|Temper*</var>	
		
		<var name="Rings">Capacity Ring|Trizek Ring|Warp Ring|Vocation Ring|Dim. Ring (Holla)|Dim. Ring (Mea)|Dim. Ring (Dem)|Facility Ring|Echad Ring</var>
		<var name="Town">Ru'Lude Gardens|Nashmau|Tavnazian Safehold|Port*|Bastok*|Windurst*|*San d'Oria|*Jeuno|*Whitegate|*Adoulin|Selbina|Mhaura|Norg</var>

		<var name="Set">Idle</var>
		<var name="DefaultSet">Idle</var>
		<var name="DualWield">false</var>
		<var name="Buffer">===============</var>
	</variables>
    
	<init>
        <if p_subjob="DNC|NIN">
            <setvar name="DualWield" value="true" />
        </if>
        <else>
            <setvar name="DualWield" value="false" />
        </else>

        <command delay="5000">/lockstyleset 4</command>
    </init>

	<inputcommands>
		<cmd input="/kill">
			<setvar name="Set" value="$DefaultSet"/>
			<addtochat color="4">$Buffer [Resetting to DefaultSet: $DefaultSet !] $Buffer</addtochat>
		</cmd>

        <cmd input="/tp">
            <if advanced="$Set!=TP">
                <setvar name="Set" value="TP" />
                <addtochat color="4">$Buffer [Equipping $Set Set!] $Buffer</addtochat>
            </if>
            <else>
                <setvar name="Set" value="$DefaultSet" />
                <addtochat color="4">$Buffer [Resetting to DefaultSet: $DefaultSet !] $Buffer</addtochat>
            </else>
        </cmd>

        <cmd input="/dt">
            <if advanced="$Set!=DT">
                <setvar name="Set" value="DT" />
                <addtochat color="4">$Buffer [Equipping $Set Set!] $Buffer</addtochat>
            </if>
            <else>
                <setvar name="Set" value="$DefaultSet" />
                <addtochat color="4">$Buffer [Resetting to DefaultSet: $DefaultSet !] $Buffer</addtochat>
            </else>
        </cmd>

	</inputcommands>
	<idlegear>
        <if eq_rring="$Rings">
            <equip lock="true">
                <rring>%eq_ring2</rring>
            </equip>
        </if>
        <if eq_ring1="$Rings">
            <equip lock="true">
                <ring1>%eq_ring1</ring1>
            </equip>
        </if>
		
		<if advanced="(%t_hpp>0)&amp;(%t_hpp<5)">
			<equip lock="true">
				<back>Aptitude Mantle +1</back>
			</equip>
		</if>

		<equip set="$Set" />
	</idlegear>
	<preranged>
	</preranged>
	<midranged>
	</midranged>
	<premagic>
		<!-- Equip the Fast Cast set first -->
		<equip set="FastCast" />

		<!-- Overwrite the gear based on certain magical spells -->
		<if ad_skill="enhancingmagic">
			<equip set="EnhanceSkill" />

			<if ad_name="Stoneskin">
				<equip set="Stoneskin"/>
			</if>		
		</if>
		<elseif advanced="%ad_name=Cure*">
			<equip set="CureCastTime"/>
		</elseif>

	</premagic>
	<midmagic>
		<if ad_name="Cursna">
			<equip set="Cursna" />
		</if>
		<elseif ad_skill="healingmagic">
			<equip set="Cure" />
		</elseif>
		<elseif ad_skill="enhancingmagic">
			<if ad_name="($EnhanceTimeOfCast)">
				<equip set="EnhanceSkill" />
			</if>
			<else>
				<equip set="EnhanceDuration" />
			</else>
		</elseif>
		<elseif ad_skill="enfeeblingmagic">
			<equip set="Enfeebling" />
		</elseif>
		<else>
			<equip set="MagicAt" />

			<if ad_element="dark">
				<equip>
					<head>Pixie Hairpin +1</head>
				</equip>
			</if>
		</else>
	</midmagic>
	<jobability>
	</jobability>
	<weaponskill>
		<equip set="Fotia"/>

		<if ad_name="Chant du Cygne">
			<equip>
				<ring2>Ramuh Ring +1</ring2>
			</equip>
		</if>
	</weaponskill>
</ashitacast>